    "prop-types": "^15.7.2",
    "css-vars-ponyfill": "^2.3.2",
    "date-fns": "^2.16.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sendbird": "^3.0.140"
  },
  "bugs": {
    "url": "https://github.com/sendbird/uikit-js/issues"
  },
  "homepage": "https://sendbird.com",
