{
  "name": "@sendbird/uikit-react",
  "version": "3.16.7",
  "description": "Sendbird UIKit for React: A feature-rich and customizable chat UI kit with messaging, channel management, and user authentication.",
  "keywords": [
    "sendbird",
    "uikit",
    "react",
    "chat",
    "messaging",
    "real-time",
    "UI components",
    "user authentication",
    "channel management",
    "SDK integration",
    "customizable",
    "feature-rich",
    "social app",
    "customer support",
    "API"
  ],
  "main": "dist/cjs/index.js",
  "style": "dist/index.css",
  "typings": "dist/index.d.ts",
  "module": "dist/index.js",
  "files": [
    "release/**/*",
    "dist/**/*",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "yarn start:apps:testing",
    "test": "jest",
    "test-update-snapshot": "jest -u",
    "test:coverage": "yarn test --coverage",
    "test:visual": "npx playwright test",
    "cp-css": "./scripts/copy-css.sh",
    "clean-release": "rm -rf ./dist",
    "build": "yarn clean-release; rollup -c && yarn build:post",
    "build:post": "node ./scripts/post_build.js;",
    "start": "rollup -c -w",
    "reset": "yarn cache clean; yarn install",
    "prepublishOnly": "yarn build",
    "predeploy": "yarn clean-release; yarn install; yarn build",
    "lint": "yarn eslint",
    "lint:fix": "yarn eslint --fix",
    "eslint": "eslint 'src/**/*.ts*'",
    "stylelint": "stylelint 'src/**'",
    "generate-component": "plop",
    "inspect": "yarn test; yarn lint;",
    "storybook": "storybook dev -p 6006",
    "start:storybook": "yarn storybook",
    "start:apps:testing": "yarn workspace @uikit-app/testing dev",
    "build:docs": "node ./scripts/build_docs.mjs",
    "build:storybook": "storybook build",
    "build:apps:testing": "yarn workspace @uikit-app/testing build"
  },
  "repository": {
    "type": "git",
    "url": "git+git@github.com:sendbird/sendbird-uikit-react-sources.git"
  },
  "author": "SendBird <support@sendbird.com>",
  "license": "SEE LICENSE IN LICENSE.md",
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "dependencies": {
    "@sendbird/chat": "^4.19.0",
    "@sendbird/react-uikit-message-template-view": "^0.0.8",
    "@sendbird/uikit-tools": "^0.0.8",
    "css-vars-ponyfill": "^2.4.9",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.6"
  },
  "bugs": {
    "url": "https://community.sendbird.com"
  },
  "homepage": "https://sendbird.com",
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/eslint-parser": "^7.27.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.27.1",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@playwright/test": "^1.52.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-eslint": "^9.0.5",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-typescript": "^12.1.2",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/manager-api": "^8.6.14",
    "@storybook/react-vite": "^9.0.6",
    "@svgr/rollup": "^8.1.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.0.0",
    "@types/use-sync-external-store": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^30.0.0",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "browserslist": "^4.25.0",
    "caniuse-lite": "^1.0.30001721",
    "eslint": "^9.28.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "global-jsdom": "^26.0.0",
    "husky": "^9.1.7",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "jest-extended": "^6.0.0",
    "jsdom": "^26.1.0",
    "plop": "^4.0.1",
    "postcss": "^8.5.4",
    "postcss-rtlcss": "^5.7.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rollup": "^4.42.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-scss": "^4.0.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass": "^1.89.2",
    "storybook": "^9.0.6",
    "stylelint": "^16.20.0",
    "stylelint-config-sass-guidelines": "^12.1.0",
    "ts-pattern": "^5.7.1",
    "typedoc": "^0.28.5",
    "typescript": "^5.8.3",
    "use-sync-external-store": "^1.5.0",
    "vite": "^6.3.5",
    "vite-plugin-svgr": "^4.3.0"
  },
  "browserslist": [
    "defaults"
  ],
  "packageManager": "yarn@3.5.0",
  "prettier": {
    "singleQuote": true,
    "printWidth": 140,
    "tabWidth": 2
  },
  "workspaces": [
    "apps/*"
  ]
}
